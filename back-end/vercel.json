{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/main.js",
      "methods": [
        "GET",
        "POST"
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/main.js"
    }
  ]
}